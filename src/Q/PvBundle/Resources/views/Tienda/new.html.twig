{% extends "QPvBundle::template.html.twig" %}
{% block title %}Tienda - Nueva{% endblock %}
{% block content %}
    
    <h1>Nueva Tienda</h1>

    
    <form action="{{ path('tienda_create') }}" method="post" {{ form_enctype(form) }}>
        {% form_theme form 'QPvBundle:Error:error_field.html.twig'%}
        {{ form_widget(form, {'attr' : {'class' : 'form_new'}}) }}
        <p>
            <button class="fsbutton_ok" type="submit"> Ok </button>
        </p>
    </form>

        <ul class="record_actions">
    <li>
        <a class="fsbutton_cancel"  href="{{ path('tiendas_index') }}">
            Cancelar
        </a>
    </li>
</ul>
{% endblock %}
